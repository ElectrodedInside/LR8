import java.util.HashSet;
import java.util.Scanner;
import java.util.Set;

public class JollyJumper {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        System.out.println("Введите последовательность в формате: n a1 a2 a3 ... an");
        System.out.println("Например: 4 1 4 2 3");
        System.out.println("Для завершения введите пустую строку");

        while (scanner.hasNextLine()) {
            String line = scanner.nextLine().trim();
            if (line.isEmpty()) {
                break;
            }

            String[] parts = line.split("\\s+");
            int n = Integer.parseInt(parts[0]);
            int[] sequence = new int[n];

            // Заполняем последовательность
            for (int i = 0; i < n; i++) {
                sequence[i] = Integer.parseInt(parts[i + 1]);
            }

            if (n == 1) {
                System.out.println("Jolly");
                continue;
            }

            Set<Integer> diffs = new HashSet<>();
            boolean isValid = true;

            // Вычисляем абсолютные разности и проверяем, что они в [1, n-1]
            for (int i = 0; i < n - 1; i++) {
                int diff = Math.abs(sequence[i] - sequence[i + 1]);
                if (diff < 1 || diff > n - 1) {
                    isValid = false;
                    break;
                }
                diffs.add(diff);
            }

            // Проверяем, что все числа от 1 до n-1 присутствуют
            if (isValid) {
                for (int i = 1; i <= n - 1; i++) {
                    if (!diffs.contains(i)) {
                        isValid = false;
                        break;
                    }
                }
            }

            System.out.println(isValid ? "Jolly" : "Not jolly");
            System.out.println("\nВведите следующую последовательность или пустую строку для выхода");
        }

        System.out.println("Программа завершена");
        scanner.close();
    }
}
